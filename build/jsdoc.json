{
  "opts": {
    "destination": "./docs/",
    "readme": "./.github/CONTRIBUTING.md",
    "recurse": true,
    "template": "./node_modules/minami"
  },
  "plugins": [
    "plugins/markdown",
    "../node_modules/jsdoc-export-default-interop/dist/index"
  ],
  "source": {
    "exclude": [
      "./src/vendor"
    ],
    "excludePattern": "(\\.component\\.|\\.e2e\\.|\\.integration\\.|\\.test\\.|\\.unit\\.)",
    "include": [
      "./src/App/App.js",
      "./src/core/",
      "./src/models/",
      "./src/services/",
      "./src/utilities/"
    ]
  },
  "sourceType": "module",
  "templates": {
    "theme": "cosmo"
  }
}
